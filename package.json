{
  "name": "naukluft-app",
  "version": "1.0.0",
  "description": "Application for Naukluft Nappe Complex mapping",
  "main": "index.js",
  "scripts": {
    "watch:main": "electron-webpack dev",
    "watch:web": "scripts/bundle.zsh",
    "compile:web": "webpack -c webpack.config.js",
    "start": "npm run watch:main",
    "compile:main": "electron-webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "yarn workspace @naukluft/web-app2 run dev",
    "dev:staging": "NAUKLUFT_API_BASE_URL='https://birdnest.geology.wisc.edu/naukluft-api' webpack --watch",
    "figures": "cd app && figment --spec ./figure-spec.js",
    "watch:api": "nodemon",
    "start:api": "cd ../api && npm run start",
    "deploy": "scripts/deploy-frontend.zsh"
  },
  "workspaces": [
    "packages/*",
    "app",
    "app2"
  ],
  "prettier": {},
  "author": "Daven Quinn",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^1.11.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@loadable/component": "^5.15.2",
    "@macrostrat/column-components": "workspace:*",
    "@macrostrat/revision-info-webpack": "^1.0.0",
    "@macrostrat/ui-components": "^1.0.5",
    "@naukluft/web-app2": "workspace:*",
    "@turf/polygonize": "^5.1.5",
    "@types/cors": "^2.8.10",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.0.1",
    "bezier-js": "^2.3.1",
    "bluebird": "^3.4.7",
    "browser-request": "^0.3.3",
    "cesium": "^1.82.1",
    "css-loader": "^3.0.0",
    "d3": "^4.4.1",
    "d3-jetpack": "^2.0.13",
    "d3-jetpack-module": "^0.0.19",
    "d3-selection-multi": "^1.0.0",
    "find-root": "^1.1.0",
    "hyperstyles": "^3.3.1",
    "js-yaml": "^3.7.0",
    "naukluft-data-backend": "workspace:*",
    "prop-types": "^15.7.2",
    "resium": "^1.13.1",
    "styled-components": "^4.0.2",
    "stylus": "^0.54.5",
    "textures": "^1.2.0"
  },
  "packageManager": "yarn@4.5.0"
}
